{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "ignore": [
    "dist/*.js"
  ],
  "env": {
    "test": {
      "presets": [
        "env"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-runtime",
        "istanbul"
      ]
    }
  }
}

