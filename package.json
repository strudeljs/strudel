{
  "name": "strudel",
  "version": "0.7.0",
  "description": "A front-end framework for back-end powered web.",
  "author": "Mateusz ≈Åuczak <mateusz.luczak@outlook.com>",
  "license": "MIT",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "setup": "node build/install-hooks.js",
    "build": "node ./build/build.js",
    "examples": "webpack-dev-server --config webpack.common.js",
    "test": "NODE_ENV=test npm run lint && npm run test:unit && npm run test:e2e && npm run test:cover",
    "test:unit": "NODE_ENV=test mocha --opts ./test/unit/mocha.opts ./test/unit/**/*.js",
    "test:e2e": "node test/e2e/runner.js",
    "test:cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test:unit",
    "test:sauce": "node test/e2e/runner.js --config ./test/e2e/sauce.config.js --test tabs --env chrome,firefox,ie11,edge,safari,android,iphone",
    "lint": "eslint src",
    "codecov": "NODE_ENV=test nyc report --reporter=lcov | codecov",
    "release": "bash build/release.sh",
    "release:notes": "node build/release-notes.js",
    "commit": "git-cz"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "main": "dist/strudel.js",
  "module": "dist/strudel.es.js",
  "files": [
    "dist/strudel.js",
    "dist/strudel.min.js",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strudeljs/strudel"
  },
  "bugs": "https://github.com/strudeljs/strudel/issues",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.32.3",
    "codecov": "^2.1.0",
    "commitizen": "^2.10.1",
    "conventional-changelog": "^2.0.1",
    "cross-spawn": "^5.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "http-server": "^0.10.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "nightwatch": "^0.9.14",
    "nightwatch-helpers": "^1.2.0",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-replace": "^2.0.0",
    "saucelabs": "^1.4.0",
    "selenium-server": "^3.3.1",
    "shelljs": "^0.7.8",
    "uglify-js": "^2.8.22",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
